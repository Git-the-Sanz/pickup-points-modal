{
  "name": "pickup-points-modal",
  "version": "0.1.1",
  "description": "Pickup Points Modal React component",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "nwb build-react-component --no-demo && npm run removeMocks && npm run addLocales",
    "removeMocks": "rm -rf lib/**/__mocks__ && rm -rf lib/__mocks__",
    "addLocales": "cp -R -f src/locales/ lib/locales/",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-component",
    "vtex",
    "checkout",
    "pickup-points"
  ],
  "repository": "git@github.com:vtex/pickup-points-modal.git",
  "author": "Fernando de Almeida Coelho <nandoacoelho@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@vtex/address-form": "^1.4.3",
    "lodash": "^4.17.5"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-preset-react-app": "^3.1.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.17.0",
    "eslint-config-vtex": "^8.0.0",
    "eslint-config-vtex-react": "^3.0.3",
    "eslint-config-vtex-react-app": "^3.1.1",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "i18n-iso-countries": "^3.5.0",
    "jest": "^22.2.2",
    "jest-enzyme": "^4.2.0",
    "nwb": "^0.21.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-intl": "^2.4.0",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/umd/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/umd/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]lib[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]umd[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]es[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)(\\?.*)?$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  }
}
